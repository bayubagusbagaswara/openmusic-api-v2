{
  "editor.formatOnSave": true,
  "editor.lineHeight": 25,
  "editor.minimap.enabled": false,
  "editor.fontSize": 13,
  "editor.fontLigatures": false,
  "editor.fontFamily": " 'Jetbrains Mono', Consolas, 'Courier New', monospace ",
  "editor.wordWrap": "on",
  "terminal.integrated.fontSize": 13,
  "workbench.colorTheme": "One Dark Pro",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.colorCustomizations": {
    "editor.background": "#171717",
    "editorLineNumber.activeForeground": "#35d7dd",
    "editor.selectionBackground": "#366ea350",
    "editor.lineHighlightBackground": "#63636331",
    "tab.activeBackground": "#171717",
    "tab.hoverBackground": "#171717",
    "tab.hoverForeground": "#ffff",
    "activityBarBadge.background": "#16d8df9c"
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.validate": ["javascript"]
}
